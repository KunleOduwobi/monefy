<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Android Mobile 14 - Smoke Tests">
	<listeners>
		<listener class-name="utils.Listeners" />
		<listener class-name="utils.AnnotationTransformer" />
	</listeners>
	<parameter name="NoReset" value="false"></parameter>
	<parameter name="BStackDevice" value="SM-A525F"></parameter>
	<parameter name="BStackDeviceOS" value="14.0"></parameter>
	<parameter name="geoLocation" value="FR"></parameter>
	<parameter name="networkProfile" value="4g-lte-high-latency"></parameter>
	<parameter name="deviceOrientation" value="landscape"></parameter>
	<parameter name="locale" value="FR"></parameter>
	<parameter name="language" value="en"></parameter>
	<parameter name="deviceType" value="mobile"></parameter>


	<test thread-count="5" name="Dashboard Validation Test">
		<parameter name="NoReset" value="false"></parameter>
		<parameter name="Currency" value="£"></parameter>
		<classes>
			<class name="androidTests.DashboardValidation" />
		</classes>
	</test>
	<test thread-count="5" name="Add Income Test">
		<parameter name="NoReset" value="false"></parameter>
		<parameter name="Currency" value="£"></parameter>
		<classes>
			<class name="androidTests.AddIncome" />
		</classes>
	</test>


</suite>